{
  "name": "vtscada-function-retrieval",
  "displayName": "VTScada Function Retrieval",
  "description": "",
  "version": "0.0.1",
  "publisher": "vtscada",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "files": [
    "resources/model.en.onnx",
    "resources/model.zh-cn.onnx",
    "resources/model.zh-tw.onnx",

    "resources/model_info.en.json",
    "resources/model_info.zh-cn.json",
    "resources/model_info.zh-twjsonx",
    
    "resources/tfidf_matrix.onnx",
    "resources/model_info.json"
  ],
  "activationEvents": [
    "onLanguage:vtscadascript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "vtscadascript",
        "aliases": [
          "VTScada Script"
        ],
        "extensions": [
          ".SRC",
          ".WEB"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.inline-completion-settings",
        "title": "Completion Settings"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./"
  },
  "dependencies": {
    "natural": "^8.1.0",
    "onnxruntime-node": "^1.23.0",
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.6.2"
  }
}
