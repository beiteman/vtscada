{
  "name": "vtscada-lsp-retrieval",
  "displayName": "VTScada Function Retrieval",
  "description": "",
  "version": "0.0.1",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:vtscadascript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "vtscadascript",
        "aliases": [
          "VTScada Script"
        ],
        "extensions": [
          ".SRC",
          ".WEB"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.inline-completion-settings",
        "title": "Completion Settings"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.6.2"
  }
}